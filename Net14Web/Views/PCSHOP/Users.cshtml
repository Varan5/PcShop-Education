@using Net14Web.Models.PcShop
@model List<UserViewModel>

@{
    Layout = "_LayoutPcShop";
}

<div>
    <table>
        <tr><th>Id</th><th>Логин</th><th>Имя</th><th></th></tr>
        @foreach (var User in Model)
        {
            <tr><td>@User.Id</td><td>@User.Login</td><td>@User.FirstName</td><td><a href="/PCSHOP/DeleteUsers?id=@User.Id" class="remove-link">Удалить</a></td></tr>
        }
    </table>
    
</div>